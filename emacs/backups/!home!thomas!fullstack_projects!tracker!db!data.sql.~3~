\echo 'Loading data'

\set VERBOSITY terse
\set ON ERROR STOP true

set search_path = data, public;

\echo 'Store exercises'
insert into exercises
       (exercise_name)
values ('Chest press'),
       ('Rack chins'),
       ('Lunges');


\echo 'Store time series data'
insert into measurements
       (measurement_date, calories, protein, carbohydrates, fat, weight)
values ('2021-12-01', 2000, 140, 80, 80, 80),
       ('2021-12-02', 2000, 140, 80, 80, 80),
       ('2021-12-03', 2000, 140, 80, 80, 80),
       ('2021-12-04', 2000, 140, 80, 80, 80);


\echo 'Store workouts'
insert into workouts
       (workout_name, workout_date)
values ('workout 1', '2021-12-01'::date),
       ('workout 2', '2021-12-02'::date);

insert into workout_stages
       (workout_stage_number, workout_id)
values (1, 1),
       (2, 1),
       (1, 2),
       (2, 2);

insert into sets
       (set_number, set_exercise, set_weight, set_reps, workout_stage_id)
values (1, 1, 50, 10, 1),
       (2, 1, 50, 10, 1),
       (3, 1, 50, 10, 1),
       (1, 2, 50, 10, 2),
       (2, 2, 50, 10, 2),       
       (3, 2, 50, 10, 2),
       (1, 1, 50, 10, 3),
       (2, 1, 50, 10, 3),
       (3, 1, 50, 10, 3),
       (1, 2, 50, 10, 4),
       (2, 2, 50, 10, 4),       
       (3, 2, 50, 10, 4);       

